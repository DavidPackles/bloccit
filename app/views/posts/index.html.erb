<h1>All Posts</h1>
<% @posts.each do |post| %>
   <div class="media">
     <div class="media-body">
       <h4 class="media-heading">
         <%= link_to post.title, post %>
       </h4>
       <p>
         <%= truncate(post.body, length: 30, separator: ' ' ) %>
       </p>
     </div>
   </div>
<% end %>